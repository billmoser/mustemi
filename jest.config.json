
 {
    "setupFiles": ["<rootDir>/test/setup-tests.js"],
    "testRegex": "/*.test.js$",
    "collectCoverage": true,
    "coverageReporters": ["lcov", "text"],
    "coverageDirectory": "test-coverage",
    "coverageThreshold": {
        "global": {
            "branches": 0,
            "functions": 0,
            "lines": 0,
            "statements": 0
        }
    },
    "moduleDirectories": ["node_modules", "src"]
}